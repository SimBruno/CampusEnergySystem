/*---------------------------------------------------------------------------------------------------------------------------------------
Load the main model and its data
---------------------------------------------------------------------------------------------------------------------------------------*/
reset;
model moes.mod;
data moes.dat;
/*---------------------------------------------------------------------------------------------------------------------------------------
Including all technologies files 
---------------------------------------------------------------------------------------------------------------------------------------*/ 
include moesPV.mod;

include moesboiler.dat;
include moesPV.dat;

/*---------------------------------------------------------------------------------------------------------------------------------------
AMPL options
---------------------------------------------------------------------------------------------------------------------------------------*/
option solver 'gurobi';                                         # choose the solver
option omit_zero_rows 1;										# to not display 0 in rows
option omit_zero_cols 1;										# to not display 0 in columns
option presolve 5e-05;
/*---------------------------------------------------------------------------------------------------------------------------------------
Solve the problem
---------------------------------------------------------------------------------------------------------------------------------------*/
solve;

/*---------------------------------------------------------------------------------------------------------------------------------------
Display commands
---------------------------------------------------------------------------------------------------------------------------------------*/
display Edemand;
display Qheating;
display Qheatingdemand;

display mult;
display mult_t;

display use;
display use_t;

#display Flowin;
#display Flowout;

#display FlowInUnit;
#display FlowOutUnit;

#display Buildings;
